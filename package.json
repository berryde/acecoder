{
	"name": "individual-project",
	"version": "0.0.1",
	"license": "UNLICENSED",
	"scripts": {
		"dev": "yarn run build-template && rollup -c && svelte-kit dev",
		"build": "yarn run build-template && rollup -c && svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json --threshold warning",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .eslintignore --check --plugin-search-dir=. . && eslint --ignore-path .eslintignore . --fix",
		"format": "prettier --ignore-path .eslintignore --write --plugin-search-dir=. .",
		"storybook": "start-storybook -p 6006",
		"emulator": "firebase emulators:start --import=./.firebase --export-on-exit",
		"build-storybook": "build-storybook",
		"build-template": "node scripts/generateTemplate.js",
		"test": "jest src",
		"test:watch": "yarn run test -- --watch"
	},
	"devDependencies": {
		"@babel/core": "^7.15.8",
		"@codemirror/closebrackets": "^0.19.0",
		"@codemirror/commands": "^0.19.5",
		"@codemirror/comment": "^0.19.0",
		"@codemirror/fold": "^0.19.1",
		"@codemirror/gutter": "^0.19.3",
		"@codemirror/history": "^0.19.0",
		"@codemirror/lang-css": "^0.19.3",
		"@codemirror/lang-html": "^0.19.3",
		"@codemirror/lang-javascript": "^0.19.2",
		"@codemirror/lang-json": "^0.19.1",
		"@codemirror/search": "^0.19.2",
		"@codemirror/state": "^0.19.2",
		"@codemirror/theme-one-dark": "^0.19.0",
		"@codemirror/view": "^0.19.9",
		"@rollup/plugin-commonjs": "^21.0.1",
		"@rollup/plugin-node-resolve": "^13.0.6",
		"@rollup/plugin-typescript": "^8.3.0",
		"@storybook/addon-a11y": "^6.3.12",
		"@storybook/addon-actions": "^6.3.12",
		"@storybook/addon-essentials": "^6.3.12",
		"@storybook/addon-links": "^6.3.12",
		"@storybook/addon-postcss": "^2.0.0",
		"@storybook/addon-svelte-csf": "^1.1.0",
		"@storybook/svelte": "^6.3.12",
		"@sveltejs/adapter-netlify": "^1.0.0-next.35",
		"@sveltejs/kit": "next",
		"@testing-library/jest-dom": "^5.15.0",
		"@testing-library/svelte": "^3.0.3",
		"@tsconfig/svelte": "^2.0.1",
		"@types/file-saver": "^2.0.4",
		"@types/prettier": "^2.4.1",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"autoprefixer": "^10.3.7",
		"babel-jest": "^27.3.1",
		"babel-loader": "^8.2.3",
		"cssnano": "^5.0.8",
		"dotenv": "^10.0.0",
		"encoding": "^0.1.13",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"firebase": "^9.5.0",
		"firebase-admin": "^10.0.0",
		"jest": "^27.3.1",
		"netlify-cli": "^7.1.0",
		"postcss": "^8.3.11",
		"postcss-load-config": "^3.1.0",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"storybook-builder-vite": "^0.1.3",
		"storybook-dark-mode": "^1.0.8",
		"storybook-tailwind-dark-mode": "^1.0.11",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.8",
		"svelte-icons": "^2.1.0",
		"svelte-jester": "^2.1.5",
		"svelte-loader": "^3.1.2",
		"svelte-preprocess": "^4.9.8",
		"tailwindcss": "^2.2.16",
		"ts-jest": "^27.0.7",
		"tslib": "^2.3.1",
		"typescript": "^4.4.4",
		"vercel": "^23.1.2"
	},
	"type": "module",
	"dependencies": {
		"@babel/standalone": "^7.15.8",
		"file-saver": "^2.0.5",
		"jszip": "^3.7.1"
	},
	"jest": {
		"transform": {
			"^.+\\.js$": "babel-jest",
			"^.+\\.svelte$": [
				"svelte-jester",
				{
					"preprocess": true
				}
			],
			"^.+\\.ts$": "ts-jest"
		},
		"moduleFileExtensions": [
			"js",
			"ts",
			"tsx",
			"svelte"
		],
		"moduleNameMapper": {
			"^src/(.*)$": "<rootDir>/src/$1"
		},
		"setupFilesAfterEnv": [
			"@testing-library/jest-dom/extend-expect"
		],
		"testEnvironment": "jsdom",
		"transformIgnorePatterns": [
			"node_modules/(?!(svelte-icons)/)"
		]
	}
}